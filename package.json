{
  "name": "dex-subgraph",
  "version": "0.1.0",
  "scripts": {
    "abi": "node src/scripts/fetchAbi",
    "compile": "truffle compile",
    "create": "graph create anxolin/dfusion --node https://api.thegraph.com/deploy/",
    "create-local": "graph create anxolin/dfusion --node http://127.0.0.1:8020",
    "codegen": "graph codegen",
    "build": "graph build",
    "deploy": "graph deploy --debug anxolin/dfusion --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/",
    "deploy-local": "graph deploy anxolin/dfusion --ipfs http://localhost:5001 --node http://127.0.0.1:8020",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src"
  },
  "dependencies": {
    "@gnosis.pm/dex-contracts": "^0.1.2"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.16.0",
    "@graphprotocol/graph-ts": "^0.16.0",
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "truffle": "^5.1.3",
    "truffle-contract": "^4.0.31",
    "truffle-hdwallet-provider": "^1.0.4"
  }
}
